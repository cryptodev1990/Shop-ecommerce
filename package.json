{
  "name": "website",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --hmr",
    "build": "ng build",
    "prod": "ng build --configuration production",
    "watch": "ng build --watch --configuration development",
    "stage": "ng build --configuration stage",
    "test": "ng test",
    "dev:ssr": "ng run website:serve-ssr",
    "serve:ssr": "node dist/website/server/main.js",
    "build:ssr": "ng build && ng run website:server",
    "prerender": "ng run website:prerender",
    "compodoc:build": "compodoc -p tsconfig.doc.json --theme material",
    "compodoc:build-and-serve": "compodoc -p tsconfig.doc.json -s --theme material",
    "compodoc:serve": "compodoc -s --theme material",
    "lint": "ng lint --fix",
    "lint:style": "stylelint 'src/**/*.less'",
    "generate:models": "node ./generate/generate-models/generate-models.js",
    "generate:envFile": "node ./generate/generate-env/index.js",
    "analyze": "npm run ng-high-memory build -- --source-map",
    "analyze:view": "source-map-explorer dist/**/*.js"
  },
